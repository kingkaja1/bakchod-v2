# Firebase configuration - copy to .env and fill in your values
# Get these from Firebase Console > Project Settings > General > Your apps

VITE_FIREBASE_API_KEY=your_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# Optional: For push notifications (FCM). Get from Firebase Console > Project Settings > Cloud Messaging > Web Push certificates
VITE_FIREBASE_VAPID_KEY=your_vapid_key

# Optional: For GIF search. Get free key at https://developers.giphy.com/dashboard
VITE_GIPHY_API_KEY=your_giphy_api_key
